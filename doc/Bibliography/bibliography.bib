@InProceedings{PRIME_plug_and_produce,
	author="Rocha, Andr{\'e} Dion{\'i}sio
	and Barata, Diogo
	and Di Orio, Giovanni
	and Santos, Tiago
	and Barata, Jos{\'e}",
